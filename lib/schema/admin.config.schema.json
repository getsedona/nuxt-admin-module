{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "title"
  ],
  "properties": {
    "title": {
      "type": "string",
      "description": "Admin Panel Title",
      "default": "Admin"
    },
    "items": {
      "type": "array",
      "description": "Admin Menu Items",
      "default": [],
      "items": {
        "$ref": "./menu-item.schema.json"
      }
    }
  }
}
